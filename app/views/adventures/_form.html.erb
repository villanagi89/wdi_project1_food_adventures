<%= form_for [@journal, @adventure] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :meal_description %>
  <%= f.text_field :meal_description %>

  <%= f.submit %>

<% end %>
